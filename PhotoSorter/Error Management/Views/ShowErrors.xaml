<Window x:Class="PhotoForce.Error_Management.ShowErrors"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Error Log" Height="429" Width="500" ResizeMode="NoResize" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid">
    <Grid>
        <DataGrid CanUserAddRows="False" ItemsSource="{Binding errorLogData,Mode=TwoWay}" AutoGenerateColumns="True" HorizontalAlignment="Stretch" Margin="10,10,10,10" Name="dgErrorLog" VerticalAlignment="Stretch" CanUserDeleteRows="False" />
    </Grid>
</Window>
