<Window x:Class="PhotoForce.WorkPlace.SyncDeleteMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:MVVM="clr-namespace:PhotoForce.MVVM"
        MVVM:DialogCloserExtension.DialogResult="{Binding DialogResult}"
        Title="Synchronize Delete Images"  Height="33"  Width="600" WindowStyle="None" WindowStartupLocation="CenterScreen"  Background="White">
    <Grid>
        <ProgressBar Height="20" HorizontalAlignment="Stretch" Name="pbSyncDeleteMessage" 
                     Value="{Binding currentProgress}" Minimum="{Binding minValue}" Maximum="{Binding maxValue}" VerticalAlignment="Top" />
    </Grid>
</Window>
